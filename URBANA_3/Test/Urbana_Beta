    "format": "png"
}, 'Classification');

Map.addLayer(imageDMSP2, {}, 'imageDMSP2')
var biomas = ee.FeatureCollection(assetBiomasOperativos);

// Create an empty image into which to paint the features, cast to byte.
var empty = ee.Image().byte();

// Paint all the polygon edges with the same number and width, display.
var outline = empty.paint({
    featureCollection: biomas,
    color: 1,
    width: 2
});

Map.addLayer(outline, {
    palette: '#070707'
}, 'Biomas Operativos',false);

//Map.centerObject(params.geometry, 7);


var imageName = params.pais + "_" + params.region + "_" + params.gridName + "_" + params.t0.split('-')[0] + "_0";


print(imageName);

// Export Metadata for asset.

classification = classification.set('year', params.year)
    .set('grid_name', params.gridName)
    .set('pais', params.pais.toUpperCase())
    .set('region', params.region)
    .set('t0', params.t0)
    .set('t1', params.t1)
    .set('cloud_cover', params.cloudcover)
    .set('sensor', params.sensor);

Export.image.toAsset({
    "image": classification,
    "description": imageName,
    "assetId": assetOutputClass + "/" + imageName,//
    "region": params.geometry,
    "scale": 30,
    "maxPixels": 1e13,
    "pyramidingPolicy": {
        ".default": "mode"
    },
});



