params.geometry = ee.FeatureCollection(regions)
                  .filterMetadata("id_regionC","equals", params.region);
Map.centerObject(params.geometry,10)
//params.geometry = ee.FeatureCollection("USDOS/LSIB/2013").filterMetadata('cc','equals','PE'); 


var col = new Collection(params);
var sma = new SMA(col.getCollection(), params);
var mask = new Mask(sma.getCollection(), params);
var ndfi = new NDFI(mask.getCollection(), params);
var ind = new Indexes(ndfi.getCollection(), params);

var collection = ind.getCollection();

collection = collection.filterDate(params.t0, params.t1);

var comp = new Composite(collection);

var composite = comp.Median()
    .addBands({
        "srcImg": mask.terrain,
        "names": ["slope"],
        "overwrite": true
    })
    .addBands({
        "srcImg": mask.terrain,
        "names": ["elevation"],
        "overwrite": true
    });

var dt = new DecisionTree(composite, dtree);

var classification = dt.getData().clip(params.geometry);
classification = classification.eq(24).multiply(24).selfMask()
